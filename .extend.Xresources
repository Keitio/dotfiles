! URxvt SETUP HERE !
URxvt.internalBorder: 6
!URxvt.font: xft:monofur:size=12
URxvt.font: xft:LiberationMono:size=12
URxvt.geometry: 80x20
URxvt.depth: 32
URxvt.background:	[60]#25252c
URxvt*scrollBar:                      true
